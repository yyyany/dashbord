# Variables d'environnement pour le dashboard personnel
# Renommez ce fichier en .env et complétez les valeurs

# Configuration du serveur
PORT=5000
NODE_ENV=development

# Base de données
MONGODB_URI=mongodb://localhost:27017/dashboard

# Clés API
OPENWEATHERMAP_API_KEY=votre_cle_api
GOOGLE_CALENDAR_CLIENT_ID=votre_client_id
GOOGLE_CALENDAR_CLIENT_SECRET=votre_client_secret

# JWT Secret
JWT_SECRET=change_this_to_a_secure_random_string 